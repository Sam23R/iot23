// Relay toggle test
const int relayPin = 7;

void setup() {
  pinMode(relayPin, OUTPUT);
  digitalWrite(relayPin, LOW); // start OFF
}

void loop() {
  digitalWrite(relayPin, HIGH);
  delay(2000);
  digitalWrite(relayPin, LOW);
  delay(2000);
}

componets:
Arduino 1
jumper cable 6
bread board 1
dc moter 1
relay spdt 1
9v battery 1
